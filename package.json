{
  "name": "@etsoo/editor",
  "version": "1.0.61",
  "description": "ETSOO Free WYSIWYG HTML Editor",
  "main": "lib/cjs/index.js",
  "module": "lib/mjs/index.js",
  "types": "lib/mjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/mjs/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "start": "webpack serve",
    "build": "webpack --mode production --no-devtool",
    "webpack": "webpack",
    "release": "rollup --config rollup.config.js",
    "copy": "copyfiles -u 1 ./src/*.css ./src/components/*.css ./lib/ && echo 'CSS files copied'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ETSOO/EOEditor.git"
  },
  "keywords": [
    "EOEditor",
    "亿速编辑器",
    "TypeScript",
    "ETSOO",
    "青岛亿速思维",
    "上海亿商"
  ],
  "author": "Garry Xiao",
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie > 0",
      "chrome > 67",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/pica": "^9.0.5",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "rollup": "^4.52.5",
    "rollup-plugin-postcss": "^4.0.2",
    "terser-webpack-plugin": "^5.3.14",
    "typescript": "^5.9.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "@etsoo/shared": "^1.2.79",
    "fabric": "^6.7.1",
    "pica": "^9.0.1"
  }
}
